    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "is-empty": "^1.2.0",
    "jimp": "^0.10.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.9",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
