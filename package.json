{
  "name": "call-center",
  "version": "1.0.0",
  "description": "MERN Stack app with Redux & JWT",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
